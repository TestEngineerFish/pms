<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android">

    <data>

    </data>

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="@dimen/px_80">

        <ImageView
            android:id="@+id/item_select_people_child"
            android:layout_width="@dimen/px_14"
            android:layout_height="@dimen/px_29"
            android:layout_centerVertical="true"
            android:layout_alignParentLeft="true"
            android:src="@drawable/level"
            android:layout_marginLeft="@dimen/px_50" />

        <TextView
            android:id="@+id/item_select_child_txt"
            android:layout_width="wrap_content"
            android:layout_height="@dimen/px_80"
            android:gravity="center"
            android:layout_toRightOf="@id/item_select_people_child"
            android:layout_marginLeft="@dimen/px_24"
            android:textSize="@dimen/px_30"
            android:textColor="@color/blackTextColor"
            android:text="" />
    </RelativeLayout>
</layout>